local DarkraiX = loadstring(game:HttpGet("https://pastebin.com/raw/kQHttzpa", true))()

local Icon = Instance.new("ImageButton")
local Gui = Instance.new("ScreenGui")
local Corner = Instance.new("UICorner")

Gui.Parent = game.CoreGui
Icon.Name = "Icon"
Icon.Parent = Gui
Icon.BackgroundColor3 = Color3.fromRGB(39, 39, 39)
Icon.Position = UDim2.new(0.0464059711, 0, 0, 0)
Icon.Size = UDim2.new(0, 40, 0, 40)
Icon.Image = "http://www.roblox.com/asset/?id=12462028268"
Icon.Draggable = true
Corner.Parent = Icon

Icon.MouseButton1Click:Connect(function()
    DarkraiX:ToggleUi()
end)

local Library = DarkraiX:Window("  Sonic Hub","Ability Wars","",Enum.KeyCode.RightControl);

Tab1 = Library:Tab("Main")
Tab1:Seperator("MAIN")

Tab1:Slider("Hit Box Range :",1,10000,20,function(value)
if _G.Disabled == true then
       while true do
            if _G.Disabled == false then
                break
            end
            _G.HeadSize = value game:GetService('RunService').RenderStepped:connect(function() if _G.Disabled then for i,v in next, game:GetService('Players'):GetPlayers() do if v.Name ~= game:GetService('Players').LocalPlayer.Name then pcall(function() v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize) v.Character.HumanoidRootPart.Transparency = 0.9 v.Character.HumanoidRootPart.BrickColor = BrickColor.new("Really white") v.Character.HumanoidRootPart.Material = "Neon" v.Character.HumanoidRootPart.CanCollide = false end) end end end end)
            wait(0.8)
            end
    else
    _G.HeadSize = value game:GetService('RunService').RenderStepped:connect(function() if _G.Disabled then for i,v in next, game:GetService('Players'):GetPlayers() do if v.Name ~= game:GetService('Players').LocalPlayer.Name then pcall(function() v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize) v.Character.HumanoidRootPart.Transparency = 0.9 v.Character.HumanoidRootPart.BrickColor = BrickColor.new("Really white") v.Character.HumanoidRootPart.Material = "Neon" v.Character.HumanoidRootPart.CanCollide = false end) end end end end)
    end
end)

Tab1:Toggle("Toggle Hit Box",false,function(state)
if state then
        _G.Disabled = true
    else
       _G.Disabled = false
    end
end)

Tab1:Slider("Speed :",1,500,20,function(value)
        if _G.WsLoop == true then
       while true do
            if _G.WsLoop == false then
                break
            end
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
            wait(0.8)
            end
    else
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
    end
end)

Tab1:Toggle("Toggle Speed",false,function(state)
if state then
        _G.WsLoop = true
    else
        _G.WsLoop = false
    end
end)

Tab1:Slider("Jump :",1,500,20,function(value)
if _G.JpLoop == true then
       while true do
            if _G.JpLoop == false then
                break
            end
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
            wait(0.8)
            end
    else
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
    end
end)

Tab1:Toggle("Toggle Jump Power",false,function(state)
if state then
        _G.JpLoop = true
    else
        _G.JpLoop = false
    end
end)

Tab1.Line()

Tab1:Toggle("Esp Player",false,function()
loadstring(game:HttpGet("https://pastebin.com/raw/cCCX66GY"))()
end)

Tab1:Toggle("Anti Void",false,function(state)
if state then
	arenaVoid.CanCollide = true
	arenaVoid.Anchored = true
	arenaVoid.Transparency = 0.5
    else
	arenaVoid.CanCollide = false
	arenaVoid.Anchored = true
	arenaVoid.Transparency = 1
    end
end)

