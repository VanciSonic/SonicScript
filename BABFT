local DarkraiX = loadstring(game:HttpGet("https://pastebin.com/raw/kQHttzpa", true))()

local Icon = Instance.new("ImageButton")
local Gui = Instance.new("ScreenGui")
local Corner = Instance.new("UICorner")

Gui.Parent = game.CoreGui
Icon.Name = "Icon"
Icon.Parent = Gui
Icon.BackgroundColor3 = Color3.fromRGB(39, 39, 39)
Icon.Position = UDim2.new(0.0464059711, 0, 0, 0)
Icon.Size = UDim2.new(0, 40, 0, 40)
Icon.Image = "http://www.roblox.com/asset/?id=12462028268"
Icon.Draggable = true
Corner.Parent = Icon

Icon.MouseButton1Click:Connect(function()
    DarkraiX:ToggleUi()
end)

local Library = DarkraiX:Window("  Sonic Hub","Build A Boat For Treasure","",Enum.KeyCode.RightControl);

Tab1 = Library:Tab("Main")
Tab1:Seperator("MAIN")

Tab1:Slider("Speed :",1,500,20,function(value)
        if _G.WsLoop == true then
       while true do
            if _G.WsLoop == false then
                break
            end
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
            wait(0.8)
            end
    else
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
    end
end)

Tab1:Toggle("Toggle Speed",false,function(state)
if state then
        _G.WsLoop = true
    else
        _G.WsLoop = false
    end
end)

Tab1:Slider("Jump :",1,5000,20,function(value)
if _G.JpLoop == true then
       while true do
            if _G.JpLoop == false then
                break
            end
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
            wait(0.8)
            end
    else
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
    end
end)

Tab1:Toggle("Toggle Jump Power",false,function(state)
if state then
        _G.JpLoop = true
    else
        _G.JpLoop = false
    end
end)

Tab1 = Library:Tab("Auto Farm")
Tab1:Seperator("AUTO FARM")

Tab1:Toggle("Auto Farm ( GOLD )",false,function(state)
if state then
        _G.TweenAutoFarm = true

while true do
    if _G.TweenAutoFarm == true then
    -- Preperation
    workspace.CamoZone.VoteLaunchRE:FireServer()
    local TS = game:GetService("TweenService")
    game.Workspace.Gravity = 0
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-64, 26, -183)
    
    -- Informations 
    local InformationLong = TweenInfo.new(
        20, -- Time
        Enum.EasingStyle.Linear, -- EasingStyle
        Enum.EasingDirection.Out, -- EasingDirection
        0, -- RepeatCount (when less than zero the tween will loop indefinitely)
        false, -- Reverses (tween will reverse once reaching it's goal)
        0 -- DelayTime
    )
    local InformationShort = TweenInfo.new(
        1, -- Time
        Enum.EasingStyle.Linear, -- EasingStyle
        Enum.EasingDirection.Out, -- EasingDirection
        0, -- RepeatCount (when less than zero the tween will loop indefinitely)
        false, -- Reverses (tween will reverse once reaching it's goal)
        0 -- DelayTime
    )
    
    -- Tweens
    local TweenBegin = TS:Create(game.Players.LocalPlayer.Character.PrimaryPart, InformationLong, {CFrame = CFrame.new(Vector3.new(-45, 77, 8547))})
    local TweenEnd = TS:Create(game.Players.LocalPlayer.Character.PrimaryPart, InformationShort, {CFrame = CFrame.new(Vector3.new(-56, -360, 9489))})
     
    -- Playing
    TweenBegin:Play()
    task.wait(20)
    TweenEnd:Play()
    task.wait(5)
    workspace.ClaimRiverResultsGold:FireServer() 
    else
        break
    end
end

    else
        workspace.Gravity = 196.2
        _G.TweenAutoFarm = false

while true do
    if _G.TweenAutoFarm == true then
    -- Preperation
    workspace.CamoZone.VoteLaunchRE:FireServer()
    local TS = game:GetService("TweenService")
    game.Workspace.Gravity = 0
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-64, 26, -183)
    
    -- Informations 
    local InformationLong = TweenInfo.new(
        20, -- Time
        Enum.EasingStyle.Linear, -- EasingStyle
        Enum.EasingDirection.Out, -- EasingDirection
        0, -- RepeatCount (when less than zero the tween will loop indefinitely)
        false, -- Reverses (tween will reverse once reaching it's goal)
        0 -- DelayTime
    )
    local InformationShort = TweenInfo.new(
        1, -- Time
        Enum.EasingStyle.Linear, -- EasingStyle
        Enum.EasingDirection.Out, -- EasingDirection
        0, -- RepeatCount (when less than zero the tween will loop indefinitely)
        false, -- Reverses (tween will reverse once reaching it's goal)
        0 -- DelayTime
    )
    
    -- Tweens
    local TweenBegin = TS:Create(game.Players.LocalPlayer.Character.PrimaryPart, InformationLong, {CFrame = CFrame.new(Vector3.new(-45, 77, 8547))})
    local TweenEnd = TS:Create(game.Players.LocalPlayer.Character.PrimaryPart, InformationShort, {CFrame = CFrame.new(Vector3.new(-56, -360, 9489))})
     
    -- Playing
    TweenBegin:Play()
    task.wait(20)
    TweenEnd:Play()
    task.wait(5)
    workspace.ClaimRiverResultsGold:FireServer() 
    else
        break
    end
end

    end
end)

Tab1:Toggle("Auto Farm ( GOLD BLOCK )",false,function(state)
if state then
        _G.TweenAutoFarm = true

while true do
    if _G.TweenAutoFarm == true then
    -- Preperation
    workspace.CamoZone.VoteLaunchRE:FireServer()
    local TS = game:GetService("TweenService")
