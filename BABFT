end

    end
end)

Tab1 = Library:Tab("Quest")
Tab1:Seperator("QUEST")

Tab1:Button("Febbi The Duck Quest",function()
workspace.QuestMakerEvent:FireServer(100)
end)

Tab1:Button("Gift Bettle Quest",function()
workspace.QuestMakerEvent:FireServer(99)
end)

Tab1:Button("Christmas 2021",function()
workspace.QuestMakerEvent:FireServer(101)
end)

Tab1.Line()

Tab1:Button("Inner Cloud (Easter 2021)",function()
placeid = 1930863474 --Place Id
    tp = game:GetService('TeleportService')

    for i,v in pairs (game.Players:GetChildren()) do
        tp:Teleport(placeid, v)
    end
end)

Tab1:Button("The Secret Place (Halloween 2021)",function()
placeid = 1930665568 --Place Id
    tp = game:GetService('TeleportService')

    for i,v in pairs (game.Players:GetChildren()) do
        tp:Teleport(placeid, v)
    end
end)

Tab1:Button("Winter Place (Christmas 2021?)",function()
placeid = 1930866268 --Place Id
    tp = game:GetService('TeleportService')

    for i,v in pairs (game.Players:GetChildren()) do
        tp:Teleport(placeid, v)
    end
end)

